{
  "name": "Intersections_API",
  "version": "1.0.0",
  "description": "Template Application",
  "scripts": {
    "start": "NODE_ENV=uat ts-node src/app.ts",
    "build": "rm -rf ./dist && npx tsc",
    "install-dependencies": "npm i aevm bluebird body config cors @elastic/elasticsearch dotenv express @hapi/joi lodash moment morgan pg promise-mysql ts typescript && npm i -D @types/bluebird @types/express @types/lodash @types/node @types/hapi__joi tslint",
    "debug": "NODE_ENV=uat node --inspect -r ts-node/register src/app.ts",
    "pm2:uat": "npm run build && pm2 start ecosystem.config.js --env uat",
    "pm2:prod": "npm run build && pm2 start ecosystem.config.js --env production"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aevm": "^3.0.0",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.5",
    "body": "^5.1.0",
    "config": "^3.2.2",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "promise-mysql": "^4.1.0",
    "ts": "^0.2.2",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/config": "0.0.34",
    "@types/elasticsearch": "^5.0.35",
    "@types/express": "^4.17.1",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/lodash": "^4.14.140",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.7.8",
    "tslint": "^5.20.0"
  }
}
